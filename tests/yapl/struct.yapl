struct test {
    int a;
    int b;
    func add() -> int {
        return a + b;
    }
    func addA(int c) -> int {
        return a + c;
    }
}

test s(12, 5);

func f(int a, int b) -> int {
    test t(9, 0);
    t = {a, b};
    t.b = 12;

    return t.a;
}

func f1(int a) -> int {
    int b = s.add();
    return b + s.addA(a);
}

func f2(int array[2], int loops) -> void {
    for (int i in array) {
        print(i);
    }

    for (int i in 0 ... loops) {
        print(i);
    }
}

func t(int a) -> test {
    test s1(a, 2);
    return s1;
}

func t1(test t) -> test {
    test t1(1, 2);
    t1 = t;
    return t1;
}

func copy(test t) -> test {
    test r(t.a, t.b);

    return r;
}
