func add(int x, int y) -> int {
    return x + y;
}

for(int i in 0...4) {
    stdio::print(i); // 0, 1, 2, 3, 4
}

for(int i in 0..<4) {
    stdio::print(i); // 0, 1, 2, 3
}

for(int i in 0..-4) {
    stdio::print(i); // 0, -1, -2, -3
}

for(int i in 0<..4) {
    stdio::print(i); // 1, 2, 3, 4
}

